name 'test1 register' \
register test1 \
	 task dummy_module -test_ret 1

name 'test1 test' task dummy_module .cond 'test1 is changed' \
	-test_ret 1 -execute_out 'test1 executed'

name 'test2 register' \
register 'test2' \
	 task dummy_module -test_ret 1

name 'test2 test' task dummy_module .cond raw:'[ "`valueof test2`" != "changed" ]' \
	-execute_out 'test2 skipped' -test_ret 0

name 'test3 register' \
register 'test3' \
	 task dummy_module -test_ret 0

name 'test3 test' task dummy_module .cond raw:'[ `valueof test3` != "ok" ]' \
	-execute_out 'test3 skipped' -test_ret 1

name 'test4 register' \
register test4 \
	 task dummy_module -test_ret 0
