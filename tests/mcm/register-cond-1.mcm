name 'test1 register' \
register test1 \
task dummy_module -test_ret 1

name 'test1 test' \
cond 'test1 is changed' \
task dummy_module \
	-test_ret 1 -execute_out 'test1 executed'

name 'test2 register' \
register 'test2' \
task dummy_module -test_ret 1

name 'test2 test' \
cond raw:'[ "`valueof test2`" != "changed" ]' \
task dummy_module \
	-execute_out 'test2 skipped' -test_ret 0

name 'test3 register' \
register 'test3' \
task dummy_module -test_ret 0

name 'test3 test' \
cond raw:'[ `valueof test3` != "ok" ]' \
task dummy_module \
	-execute_out 'test3 skipped' -test_ret 1

name 'test4 register' \
register test4 \
task dummy_module -test_ret 0
